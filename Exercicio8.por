programa {
  funcao inicio() {
    /*Calcule a área de um retângulo*/

    //Declaração das variáveis
    inteiro a, b, r

    //Atribuição dos valores
    escreva ("Digite a altura: ")
    leia (a)
    escreva ("Digite a largura: ")
    leia (b)

    //Cálculo e o resultado final
    r = a*b
    escreva ("A área é: " +r)
  }
}
